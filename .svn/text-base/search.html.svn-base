		<script type="text/javascript">
			var myvue = new Vue({
				el: '#app',
				data: {
					layui: false,
					layuitext:'错误',
					searchtype:localStorage.getItem('searchdpeata')
				},
				methods: {
					layuiboo: function() {
						this.layui = !this.layui
					},
					opennew:function(){
						mui.openWindow({
							url:'shop.html',
							id:'shop'
						})
					}
				},
				mounted: function() {
					console.log(this.searchtype)
				},
				update: function() {
					mui.init()
				}
			})
